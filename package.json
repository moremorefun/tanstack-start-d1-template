{
  "name": "tanstack-start-d1-template",
  "type": "module",
  "description": "Full-stack React app with TanStack Start, Cloudflare D1, and KV",
  "cloudflare": {
    "label": "TanStack Start + D1 Starter",
    "products": [
      "Workers",
      "D1",
      "KV"
    ],
    "categories": [
      "starter",
      "storage"
    ],
    "bindings": {
      "DB": "D1 database for SQLite storage with Drizzle ORM",
      "KV": "KV namespace for caching and sessions"
    },
    "previewImageUrl": "https://imagedelivery.net/YOUR_PREVIEW_IMAGE_URL",
    "previewIconUrl": "https://imagedelivery.net/YOUR_ICON_URL"
  },
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "serve": "vite preview",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest run --project=unit",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --project=unit --coverage",
    "test:watch": "vitest --project=unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm format && pnpm lint:fix && pnpm test && pnpm test:e2e",
    "deploy": "pnpm run build && wrangler deploy",
    "preview": "pnpm run build && vite preview",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply my-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply my-db --remote"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.13.8",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.8.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.140.5",
    "@tanstack/react-router-devtools": "^1.140.5",
    "@tanstack/react-router-ssr-query": "^1.140.5",
    "@tanstack/react-start": "^1.140.5",
    "@tanstack/router-plugin": "^1.140.5",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.23.26",
    "jose": "^5.10.0",
    "lucide-react": "^0.559.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.6",
    "ulid": "^3.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.14",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "wrangler": "^4.53.0"
  }
}
